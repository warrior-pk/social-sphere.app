{
  "expo": {
    "name": "Sphere",
    "slug": "sphere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sphere",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "width": "128px",
      "height": "128px",
      "backgroundColor": "#010101"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.warrior.sphere"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#010101"
      },
      "package": "com.warrior.sphere"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
